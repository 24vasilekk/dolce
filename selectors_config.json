{
  "farfetch.com": {
    "name": {
      "primary": [
        "script[type='application/ld+json'] → name",
        "meta[property='og:title']",
        "title"
      ],
      "fallback": [
        "[data-testid='product-short-description']",
        ".ltr-13a5og8-Title",
        ".ltr-i980jo.el610qn0",
        "h1"
      ]
    },
    "brand": {
      "primary": [
        "script[type='application/ld+json'] → brand.name",
        "extract_from_name → first_word"
      ],
      "fallback": [
        "[data-testid='brand-name']",
        ".brand"
      ]
    },
    "price": {
      "primary": [
        "meta[property='og:price:amount']",
        "meta[property='product:custom_label_4']",
        "script[type='application/ld+json'] → offers.price"
      ],
      "fallback": [
        ".ltr-1uvoax5.ee85kz00",
        ".ltr-mie4yz.e16jhb2m0",
        "[class*='price']",
        "[class*='Price']"
      ]
    },
    "currency": {
      "primary": [
        "meta[property='og:price:currency']",
        "script[type='application/ld+json'] → offers.priceCurrency"
      ],
      "fallback": ["EUR"]
    },
    "images": {
      "primary": [
        "script[type='application/ld+json'] → image",
        "meta[property='og:image']"
      ],
      "fallback": [
        "[data-testid='product-image'] img",
        "img[src*='farfetch-contents.com']"
      ]
    },
    "sizes": {
      "primary": [
        "[data-testid='ScaledSizeSelector']",
        "[data-testid='variant-selector']"
      ],
      "fallback": [
        ".size-option",
        ".size-selector"
      ]
    },
    "color": {
      "primary": [
        "script[type='application/ld+json'] → color",
        "extract_from_name → color_pattern"
      ],
      "fallback": [
        "[data-testid='variant-selector']",
        ".color-name"
      ]
    },
    "description": {
      "primary": [
        "script[type='application/ld+json'] → description",
        "meta[name='description']"
      ],
      "fallback": [
        "[data-testid='product-information-accordion']",
        "[data-testid='product-short-description']"
      ]
    },
    "availability": {
      "primary": [
        "meta[property='og:availability']",
        "script[type='application/ld+json'] → offers.availability"
      ],
      "fallback": ["text_search → in_stock|available|out_of_stock"]
    },
    "discount_amount": {
      "primary": [
        "calculated → original_price - current_price"
      ],
      "fallback": [
        "[class*='discount']",
        "[class*='save']",
        "[class*='off']",
        ".discount-amount",
        ".savings"
      ]
    },
    "discount_percentage": {
      "primary": [
        "calculated → ((original_price - current_price) / original_price) * 100"
      ],
      "fallback": [
        "[class*='discount']",
        "[class*='percent']",
        "[class*='off']",
        ".discount-percentage",
        ".percent-off"
      ]
    },
    "catalog_selectors": {
      "product_links": [
        "[data-testid='product-card'] a[href*='/item-']",
        "a[href*='/shopping/'][href*='/item-']",
        ".product-card a",
        "a[href*='item-']"
      ],
      "pagination": [
        "a[aria-label*='next' i]",
        "a[title*='next' i]",
        ".pagination a[rel='next']",
        ".pagination .next a",
        "[data-testid*='next'] a"
      ]
    }
  },
  "bestsecret.com": {
